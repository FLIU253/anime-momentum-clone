{
  "name": "ani wallpaper generator",
  "chrome_url_overrides": {
    "newtab": "tab/tab.html"
  },
  "description": "An anime wallpaper generator with a added bonus of tracking airing dates for current anime",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "new-tab-page",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "geolocation",
    "https://wall.alphacoders.com/*"
  ]
}
